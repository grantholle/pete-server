{
  "name": "pete-server",
  "version": "1.0.0",
  "adonis-version": "4.1.0",
  "description": "The HTTP server to manage Pete",
  "main": "server.js",
  "bin": {
    "pete": "cli.js"
  },
  "scripts": {
    "start": "./node_modules/.bin/pm2 start pete.config.js",
    "stop": "./node_modules/.bin/pm2 stop pete.config.js",
    "restart": "./node_modules/.bin/pm2 restart pete.config.js",
    "logs": "./node_modules/.bin/pm2 logs --lines 1000",
    "scheduler": "node ace run:scheduler",
    "test": "node ace test",
    "dev": "cross-env NODE_ENV=development ./node_modules/.bin/webpack --config webpack.config.js --watch",
    "build": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --config webpack.config.js",
    "postinstall": "node install.js"
  },
  "keywords": [
    "movies",
    "tv shows",
    "automation",
    "torrents",
    "hornberger",
    "transmission",
    "download",
    "flexget",
    "couchpotato"
  ],
  "author": "Grant Holle <http://grantholle.com>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/auth": "^3.0.7",
    "@adonisjs/bodyparser": "^2.0.5",
    "@adonisjs/cors": "^1.0.7",
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/framework": "^5.0.9",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/lucid": "^6.1.3",
    "@adonisjs/session": "^1.0.29",
    "@adonisjs/shield": "^1.0.8",
    "@adonisjs/validator": "^5.0.6",
    "@adonisjs/vow": "^1.0.17",
    "@adonisjs/websocket": "^1.0.11",
    "@adonisjs/websocket-client": "^1.0.9",
    "adonis-scheduler": "^3.0.2",
    "commander": "^3.0.1",
    "eztv-b": "^1.1.1",
    "lodash": "^4.17.15",
    "mkdirp": "^0.5.1",
    "moment": "^2.24.0",
    "moviedb-promise": "^1.4.1",
    "nanoid": "^2.1.0",
    "parse-torrent-title": "^1.2.0",
    "pm2": "^3.5.1",
    "pushbullet": "^2.4.0",
    "rarbg": "^1.2.7",
    "sanitize-filename": "^1.6.3",
    "sqlite3": "^4.1.0",
    "svelte-routing": "^1.3.0",
    "transmission-promise": "^1.1.4",
    "yify-promise": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@tailwindcss/custom-forms": "^0.2.0",
    "autoprefixer": "^9.6.1",
    "axios": "^0.21.1",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "regenerator-runtime": "^0.13.3",
    "style-loader": "^0.23.1",
    "svelte": "^3.9.1",
    "svelte-loader": "^2.13.6",
    "tailwindcss": "^1.1.2",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7"
  },
  "autoload": {
    "App": "./app"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/grantholle/pete-server"
  }
}
